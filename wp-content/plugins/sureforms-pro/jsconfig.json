{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@Admin/*": [ "../sureforms/src/admin/*" ],
			"@Blocks/*": [ "../sureforms/src/blocks/*" ],
			"@Controls/*": [ "../sureforms/src/srfm-controls/*" ],
			"@Components/*": [ "../sureforms/src/components/*" ],
			"@Utils/*": [ "../sureforms/src/utils/*" ],
			"@Svg/*": [ "../sureforms/assets/svg/*" ],
			"@Attributes/*": [ "../sureforms/src/blocks-attributes/*" ],
			"@IncBlocks/*": [ "../sureforms/inc/blocks/*" ],
			"@Image/*": [ "images/*" ],
			"@ProComponents": ["src/components/*"], 
			"@ProAdmin/*": ["src/admin/*"], 
			"@ProAttributes/*": ["blocks-config/blocks-attributes/*"], 
			"@ProBlocks/*": ["src/blocks/*"], 
			"@ProControls/*": ["blocks-config/sureforms-pro-controls/*"], 
			"@Store/*": ["sureforms/src/store/*"], 
			"@ProScripts/*": ["scripts/*"], 
			"@ProSvg/*": ["assets/svg/*"], 
			"@PremiumComponents/*": ["/src/components/*"], 
			"@ProIncBlocks/*": ["inc/blocks/*"], 
			"@ProPackage/*": ["inc/pro/*"], 
			"@BusinessPackage/*": ["inc/business/*"], 
		},
	},
	"exclude": [
		"node_modules",
		"dist",
		"build",
		"assets"
	]
}
