{
    "name": "srfm/html",
    "title": "HTML",
    "description": "Displays a SureForms HTML Content",
    "category": "sureforms-pro",
    "keywords": ["html", "code"],
    "textdomain": "sureforms-pro",
    "supports": {
        "reusable": false,
        "html": false
    },
    "example": {
        "attributes": {
            "preview": true
        }
    },
    "attributes": {
        "preview": {
            "type": "boolean",
            "default": false
        },
        "block_id": {
            "type": "string"
        },
        "formId": {
			"type": "number",
			"default": ""
		},
        "fieldWidth": {
            "type": "number",
            "default": 100
        },
        "htmlContent": {
            "type": "string",
            "default": ""
        },
        "label": {
            "type": "string",
            "default": "HTML Block"
        },
        "isConditionalLogic": {
            "type": "boolean",
            "default": false
        },
        "slug": {
            "type": "string",
            "default": ""
        }
    }
}
