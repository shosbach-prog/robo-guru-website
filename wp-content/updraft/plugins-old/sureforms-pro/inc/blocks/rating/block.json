{
	"name": "srfm/rating",
	"title": "Rating",
	"description": "Displays a SureForms Rating Field",
	"category": "sureforms-pro",
	"keywords": ["rating", "star rating", "review", "field"],
	"textdomain": "sureforms-pro",
	"supports": {
		"reusable": false,
		"html": false
	},
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"attributes": {
		"preview": {
			"type": "boolean",
			"default": false
		},
		"block_id": {
			"type": "string"
		},
		"required": {
			"type": "boolean"
		},
		"fieldWidth": {
			"type": "number",
			"default": 100
		},
		"label": {
			"type": "string",
			"default": "Rate Us"
		},
		"help": {
			"type": "string",
			"default": ""
		},
		"showText": {
			"type": "boolean"
		},
		"iconShape": {
			"type": "string",
			"default": "star"
		},
		"errorMsg": {
			"type": "string",
			"default": ""
		},
		"formId": {
			"type": "number",
			"default": ""
		},
		"isConditionalLogic": {
			"type": "boolean",
			"default": false
		},
		"slug": {
			"type": "string",
			"default": ""
		},
		"ratingText": {
			"type": "array",
			"default": []
		},
		"thumbsText": {
			"type": "array",
			"default": []
		},
		"defaultRating": {
			"type": "number",
			"default": 0
		}
	}
}
