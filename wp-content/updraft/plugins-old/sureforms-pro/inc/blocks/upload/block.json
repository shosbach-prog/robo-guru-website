{
	"name": "srfm/upload",
	"title": "Upload",
	"description": "Displays a SureForms Upload Field",
	"category": "sureforms-pro",
	"keywords": ["upload", "field"],
	"textdomain": "sureforms-pro",
	"supports": {
		"reusable": false,
		"html": false
	},
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"attributes": {
		"preview": {
			"type": "boolean",
			"default": false
		},
		"block_id": {
			"type": "string"
		},
		"required": {
			"type": "boolean"
		},
		"fieldWidth": {
			"type": "number",
			"default": 100
		},
		"label": {
			"type": "string",
			"default": "Upload"
		},
		"fileSizeLimit": {
			"default": 10,
			"type": "number"
		},
		"allowedFormats": {
			"type": "array",
			"default": [
				{ "value": "jpg", "label": "jpg" },
				{ "value": "jpeg", "label": "jpeg" },
				{ "value": "gif", "label": "gif" },
				{ "value": "png", "label": "png" },
				{ "value": "pdf", "label": "pdf" }
			]
		},
		"multiple": {
			"type": "boolean",
			"default": false
		},
		"maxFiles": {
			"type": "number",
			"default": 2
		},
		"customFormats": {
			"type": "array",
			"default": []
		},
		"help": {
			"type": "string",
			"default": ""
		},
		"errorMsg": {
			"type": "string",
			"default": ""
		},
		"formId": {
			"type": "number",
			"default": ""
		},
		"isConditionalLogic": {
			"type": "boolean",
			"default": false
		},
		"slug": {
			"type": "string",
			"default": ""
		}
	}
}
