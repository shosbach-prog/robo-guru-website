{
  "name": "@jotforminc/wordpress-ai-chatbot-plugin",
  "version": "1.0.0",
  "description": "",
  "targets": {
    "dev": {
      "distDir": "./dist/app",
      "outputFormat": "global",
      "isLibrary": false,
      "context": "browser",
      "sourceMap": true
    },
    "build": {
      "distDir": "./lib/app",
      "outputFormat": "global",
      "isLibrary": false,
      "context": "browser",
      "sourceMap": false
    }
  },
  "type": "module",
  "browser": {
    "process": false,
    "buffer": false
  },
  "alias": {
    "process": false,
    "buffer": false
  },
  "scripts": {
    "prebuild": "rimraf .parcel-cache lib/app && pnpm checkdeps && pnpm lint",
    "checkdeps": "madge --circular src",
    "lint": "eslint src/ --fix --quiet",
    "dev": "parcel watch src/app.js --target dev",
    "build": "parcel build src/app.js --target build"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-2.0-only",
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "clipboard": "2.0.11",
    "cropperjs": "1.6.2",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.6",
    "js-beautify": "1.14.2",
    "lodash": "4.17.21",
    "marked": "^16.0.0",
    "prop-types": "^15.8.1",
    "react": "18.3.0",
    "react-color": "^2.19.3",
    "react-cropper": "2.3.3",
    "react-dom": "18.3.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.13.3",
    "@parcel/transformer-raw": "2.13.3",
    "@parcel/transformer-sass": "2.13.3",
    "@parcel/transformer-svg-react": "^2.13.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.2.13",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.14.0",
    "madge": "^8.0.0",
    "parcel": "^2.13.3",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "svgo": "^3.3.2"
  },
  "resolutions": {
    "axios": "^1.7.9"
  },
  "engines": {
    "pnpm": "9.7.1 || 8.3.1"
  },
  "packageManager": "pnpm@9.7.1"
}
