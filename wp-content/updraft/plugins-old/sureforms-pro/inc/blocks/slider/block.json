{
	"name": "srfm/slider",
	"title": "Slider",
	"description": "Displays a SureForms Slider Field",
	"category": "sureforms-pro",
	"keywords": ["number slider", "range slider", "range input", "text slider"],
	"textdomain": "sureforms-pro",
	"supports": {
		"reusable": false,
		"html": false
	},
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"attributes": {
		"preview": {
			"type": "boolean",
			"default": false
		},
		"block_id": {
			"type": "string"
		},
		"fieldWidth": {
			"type": "number",
			"default": 100
		},
		"label": {
			"type": "string",
			"default": "Slider"
		},
		"help": {
			"type": "string",
			"default": ""
		},
		"min": {
			"type": "number",
			"default": 0
		},
		"max": {
			"type": "number",
			"default": 100
		},
		"step": {
			"type": "number",
			"default": 10
		},
		"errorMsg": {
			"type": "string",
			"default": "This field is required."
		},
		"formId": {
			"type": "number",
			"default": ""
		},
		"isConditionalLogic": {
			"type": "boolean",
			"default": false
		},
		"slug": {
			"type": "string",
			"default": ""
		},
		"type": {
			"type": "string",
			"default": "number"
		},
		"options": {
			"type": "array",
			"default": [
				{
					"label": "Option 1",
					"value": "option-1"
				},
				{
					"label": "Option 2",
					"value": "option-2"
				},
				{
					"label": "Option 3",
					"value": "option-3"
				}
			]
		},
		"prefixTooltip": {
			"type": "string",
			"default": ""
		},
		"suffixTooltip": {
			"type": "string",
			"default": ""
		},
		"leftLabel": {
			"type": "string",
			"default": ""
		},
		"rightLabel": {
			"type": "string",
			"default": ""
		},
		"textDefaultValue": {
			"type": "string",
			"default": "Option 2"
		},
		"numberDefaultValue": {
			"type": "string",
			"default": "10"
		},
		"required": {
			"type": "boolean",
			"default": false
		}
	}
}
